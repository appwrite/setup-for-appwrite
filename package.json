{
  "name": "setup-for-actions",
  "version": "2.0.0",
  "license": "MIT",
  "description": "GitHub action for setup Appwrite CLI and login using API key or email and password",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write './**/*'",
    "format:check": "prettier --check './**/*'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "unit": "echo \"Error: no unit test specified\" && exit 1",
    "build": "ncc build -o dist src/index.js",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1"
  },
  "devDependencies": {
    "@vercel/ncc": "0.38.1",
    "prettier": "1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appwrite/setup-for-actions.git"
  }
}
